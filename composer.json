{
  "name": "wangyongdong/laravelcas",
  "description": "This is laravel phpcas",
  "keywords": ["php", "cas", "phpCas", "laravel", "auth"],
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "wangyongdong"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "php": ">=5.4.0",
    "illuminate/support": "5.x",
    "wangyongdong/phpCAS": "dev-master"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.5"
  },
  "repositories": {
    "packagist": {
      "type": "composer",
      "url": "https://packagist.phpcomposer.com"
    },
    "wangyongdong/phpCAS": {
      "type": "git",
      "url": "https://github.com/wangyongdong/phpCAS.git"
    }
  },
  "autoload": {
    "psr-4": {
      "Laravelcas\\Cas\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Laravelcas\\Cas\\Tests\\": "src/tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Laravelcas\\Cas\\CasServiceProvider"
      ],
      "aliases": {
        "Cas": "Laravelcas\\Cas\\Facades\\Cas"
      }
    }
  }
}
